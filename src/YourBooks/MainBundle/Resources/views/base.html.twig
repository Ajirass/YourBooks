{% extends '::base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    '@YourBooksMainBundle/Resources/public/css/*.css'
    '@YourBooksMainBundle/Resources/public/less/*.less'
    filter='lessphp, cssrewrite'
    output='css/style.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {% javascripts
    '@YourBooksMainBundle/Resources/public/js/*.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}